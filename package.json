{
  "name": "ssb-neon-keys",
  "version": "7.2.0",
  "description": "Rust-in-Node.js variant of ssb-keys",
  "main": "lib/index.js",
  "author": "Andre Staltz <andre@staltz.com>",
  "license": "AGPL-3.0",
  "dependencies": {
    "neon-cli": "^0.3.3"
  },
  "devDependencies": {
    "tape": "^3.0.3",
    "ssb-keys": "7.2.0"
  },
  "scripts": {
    "install": "npm run build && npm run setup-compat-tests",
    "setup-compat-tests": "cp -ar ./node_modules/ssb-keys/test/ ./test-compat/",
    "build": "neon build",
    "test-compat": "tape test-compat/*.js",
    "test-extra": "tape test-extra/*.js",
    "test": "npm run test-compat && npm run test-extra"
  }
}