{
  "name": "ssb-neon-keys",
  "version": "7.2.0-5",
  "description": "Rust-in-Node.js variant of ssb-keys",
  "main": "lib/index.js",
  "author": "Andre Staltz <andre@staltz.com>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staltz/ssb-neon-keys.git"
  },
  "dependencies": {
    "neon-cli": "~0.4.1",
    "node-pre-gyp": "^0.15.0",
    "node-pre-gyp-github": "^1.4.3"
  },
  "devDependencies": {
    "ssb-keys": "7.2.0",
    "tape": "^3.0.3"
  },
  "scripts": {
    "install": "if [ ! -d \"./.github\" ]; then node-pre-gyp install --fallback-to-build=false || neon build --release; fi",
    "build": "neon build --release",
    "minify": "rm -rf ./native/target",
    "package": "node-pre-gyp package",
    "upload-binary": "node-pre-gyp-github publish",
    "setup-compat-tests": "cp -RT ./node_modules/ssb-keys/test ./test-compat",
    "pretest": "neon build",
    "test-compat": "npm run setup-compat-tests && tape test-compat/*.js",
    "test-extra": "tape test-extra/*.js",
    "test": "npm run test-compat && npm run test-extra"
  },
  "binary": {
    "module_name": "index",
    "host": "https://github.com/staltz/ssb-neon-keys/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "module_path": "./native",
    "pkg_path": "."
  }
}
